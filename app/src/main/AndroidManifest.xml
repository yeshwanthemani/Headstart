<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.headstart.headstart">

    <supports-screens
        android:anyDensity="true"
        android:largeScreens="true"
        android:normalScreens="true"
        android:smallScreens="true"
        android:xlargeScreens="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat.Light.DarkActionBar">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".signupactivity"
            android:label="@string/title_activity_signupactivity" />
        <activity
            android:name=".MainActivity2Activity"
            android:label="@string/title_activity_main_activity2" />
        <activity
            android:name=".tabs"
            android:label="@string/title_activity_tabs" />
        <activity
            android:name=".Kodekrypt"
            android:label="@string/title_activity_kodekrypt" />
        <activity
            android:name=".Kodekryptsuccess"
            android:label="@string/title_activity_kodekryptsuccess" />
        <activity
            android:name=".Kodekryptexam"
            android:label="@string/title_activity_kodekryptexam" />
        <activity
            android:name=".dbview"
            android:label="@string/title_activity_dbview" />
        <activity android:name=".Kodekryptexamfinish"></activity>
    </application>

</manifest>
